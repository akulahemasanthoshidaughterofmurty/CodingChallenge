class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        
        l=len(digits)
        total=0
        res=[]
      
        for i in range(l):
                total=total*10+digits[i]
        total+=1
        print(total)
        total=str(total)
        for j in total:
                res.append(int(j))
        return res


